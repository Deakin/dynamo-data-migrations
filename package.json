{
  "name": "dynamo-data-migrations",
  "version": "1.0.0",
  "description": "A library to easily run migrations on DynamoDb with TypeScript.",
  "scripts": {
    "format": "./node_modules/.bin/prettier --write './src/**/*.{ts,js}'",
    "build": "tsc --build --clean",
    "check-types": "tsc --noEmit",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix"
  },
  "author": "",
  "license": "MIT",
  "keywords": [
    "dynamodb migrations database data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/technogise/dynamo-data-migrations.git"
  },
  "engines": {
    "node": "=16.0.0",
    "npm": "6.12.0"
  },
  "preferGlobal": true,
  "dependencies": {
    "aws-sdk": "^2.1113.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.55.0",
    "@types/node": "^17.0.23",
    "eslint": "^8.13.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    }
  }
}
